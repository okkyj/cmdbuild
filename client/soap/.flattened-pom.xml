						&lt;groupId>org.cmdbuild&lt;/groupId>
						&lt;artifactId>cmdbuild-core-all&lt;/artifactId>
						&lt;version>3.4.2-DEV-SNAPSHOT&lt;/version>
					&lt;/dependency>
					&lt;dependency>
						&lt;groupId>org.cmdbuild&lt;/groupId>
						&lt;artifactId>cmdbuild-services-soap&lt;/artifactId>
						&lt;version>3.4.2-DEV-SNAPSHOT&lt;/version>
					&lt;/dependency>
				&lt;/dependencies>
				&lt;executions>
					&lt;execution>
						&lt;id>generate-sources&lt;/id>
						&lt;phase>generate-sources&lt;/phase>
						&lt;configuration>
							&lt;className>org.cmdbuild.services.soap.Private&lt;/className>
							&lt;genWsdl>true&lt;/genWsdl>
							&lt;verbose>true&lt;/verbose>
						&lt;/configuration>
						&lt;goals>
							&lt;goal>java2ws&lt;/goal>
						&lt;/goals>
					&lt;/execution>
				&lt;/executions>
				&lt;configuration>
				&lt;/configuration>
			&lt;/plugin>
			&lt;plugin>
				&lt;groupId>org.apache.cxf&lt;/groupId>
				&lt;artifactId>cxf-codegen-plugin&lt;/artifactId>
				&lt;version>3.2.11&lt;/version>
				&lt;executions>
					&lt;execution>
						&lt;id>generate-sources&lt;/id>
						&lt;phase>generate-sources&lt;/phase>
						&lt;configuration>
							&lt;sourceRoot>C:\Data\CMDBuild-project\cmdbuild-upstream\cmdbuild-3.4.2-src\client\soap\target/generated/cxf&lt;/sourceRoot>
							&lt;wsdlOptions>
								&lt;wsdlOption>
									&lt;wsdl>C:\Data\CMDBuild-project\cmdbuild-upstream\cmdbuild-3.4.2-src\client\soap\target/generated/wsdl/Private.wsdl&lt;/wsdl>
									&lt;extraargs>
										&lt;extraarg>-frontend&lt;/extraarg>
										&lt;!-- used for compatibility with Java 6 -->
										&lt;extraarg>jaxws21&lt;/extraarg>
									&lt;/extraargs>
								&lt;/wsdlOption>
							&lt;/wsdlOptions>
						&lt;/configuration>
						&lt;goals>
							&lt;goal>wsdl2java&lt;/goal>
						&lt;/goals>
					&lt;/execution>
				&lt;/executions>
			&lt;/plugin></description>
